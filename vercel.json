{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "has": [{ "type": "host", "value": "ttsaudify.com" }],
      "dest": "https://www.ttsaudify.com/$1",
      "status": 301
    },
    { "src": "^/$", "dest": "/pages/index.html" },
    { "src": "^/subscription$", "dest": "/pages/subscription.html" },
    { "src": "^/success$", "dest": "/pages/success.html" },
    { "src": "^/demo$", "dest": "/pages/demo.html" },
    { "src": "^/email-verification$", "dest": "/pages/email-verification.html" },
    { "src": "^/privacy$", "dest": "/pages/privacy.html" },
    { "src": "^/report$", "dest": "/pages/report.html" }
  ]
}
